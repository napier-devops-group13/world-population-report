<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <!-- Relax Javadoc requirements while you iterate -->
  <suppress checks="JavadocType"/>
  <suppress checks="JavadocMethod"/>
  <suppress checks="JavadocVariable"/>

  <!-- Avoids 'final' redundancy nags on interfaces/constants -->
  <suppress checks="RedundantModifier"/>

  <!-- Example: to ignore line length inside generated files (uncomment if needed) -->
  <!-- <suppress checks="LineLength" files=".*generated.*"/> -->
</suppressions>
